<?xml version="1.0" encoding="utf-8"?>
<ivy-settings>
  <settings defaultResolver="remote"/>
  <credentials host="localhost" realm="Artifactory Realm" username="services" passwd="evalcode"/>
  <resolvers>
    <filesystem name="local">
      <ivy pattern="${ivy.settings.dir}/dist/[organisation].[artifact]-[revision].xml"/>
      <artifact pattern="${ivy.settings.dir}/dist/[organisation].[artifact]-[revision].[ext]"/>
    </filesystem>
    <ibiblio name="remote" m2compatible="true" root="http://localhost:9080/artifactory/services"/>
  </resolvers>
</ivy-settings>
